<div class="section-welcome">
  <div class="welcome_slider js-welcome_slider">
    {{#each contents}}
    <div class="welcome_slider_item"><a href="/news/show/{{_id}}" class="cover_link"></a>
      <div class="welcome_slider_img"><img src="/{{Image}}" alt="Image" /></div>
      <div class="welcome_slider_cont">
        <div class="welcome_slider_cats cats">
          <a href="/news/show/{{_id}}" class="cats_item">{{category.title}}</a>
        </div>
        <div class="welcome_slider_title h4">
          <a href="/news/show/{{_id}}">{{description}}</a></div>
        <div class="welcome_slider_infoline infoline">
          <div class="infoline_date">{{cretedAt}}</div>
          <span class="dash"></span>
          <div class="infoline_author">
            <div class="ava"><img  data-src="/{{author.avatar}}" src="/{{author.avatar}}"
                class="js-img" alt="avatar" />
            </div>
            {{author.name}}
          </div>
        </div>
      </div>
    </div>
    {{/each}}
  </div>
</div>
</div>
<section class="section-stories">
  <div class="wrapper">
    <div id="recent" class="recent_wrap">
      <div class="section_title">
        <h2>Recent stories</h2>
      </div>
      <div class="columns">
        <div class="columns_center">
          <div class="recent">
            {{#each recentStories}}
            <div class="recent_item">
              <div class="recent_item_img">
                <a href="/news/show/{{_id}}"><img data-src="/{{Image}}"
                    src="/{{Image}}" class="js-img" alt="" /></a>
                <div class="recent_item_cats cats"><a href="/news/show/{{_id}}" class="cats_item">{{category.title}}</a></div>
              </div>
              <div class="recent_item_cont">
                <div class="recent_item_title h3"><a href="/news/show/{{_id}}">{{title}}</a></div>
                <div class="recent_item_txt">{{description}}</div>
                <div class="recent_item_infoline infoline">
                  <div class="infoline_date">{{cretedAt}}</div>
                  <span class="dash"></span>
                  <div class="infoline_author">
                    <div class="ava"><img data-src="{{author.avatar}}"
                        src="{{author.avatar}}" class="js-img" alt="" /></div>
                      {{author.name}}
                  </div>
                  <a href="article.html" class="btn-arr"><span class="icon-arrow"></span></a>
                </div>
              </div>
            </div>
            {{/each}}
          </div>
        </div>
        <aside class="columns_sidebar sidebar">
          <div class="sidebar_widget">
            <div class="stories">
              {{#each otherNews}}

                <div class="stories_item">
                  <div class="stories_item_img"><a href="/news/show/{{_id}}"><img data-src="/{{Image}}"
                        src="/{{Image}}" class="js-img img-fluid" alt="no-image" /></a></div>
                  <div class="stories_infoline infoline d-flex justify-content-between flex-row">
                    <div class="infoline_date">{{cretedAt}}</div><br/>
                    <span class="ml-2">{{author.name}}</span>
                    <div class="infoline_tag">#{{category.title}}</div>
                  </div>
                  <div class="stories_item_title"><a href="/news/show/{{_id}}">{{title}}</a></div>
                </div>
              {{/each}}
            </div>
            <div class="stories_btn"><a href="/all" class="btn">view all</a></div>
          </div>
        </aside>
      </div>
    </div>
  </div>
</section>
